{
  "$schema": "http://json-schema.org/schema",
  "id": "MyFullSchematicsSchema",
  "title": "My Full Schematics Schema",
  "type": "object",
  "properties": {
    "index": {
      "type": "number",
      "default": 1
    },
    "name": {
      "type": "string",
      "x-prompt": "What should we name the files?"
    },
    "module": {
      "type": "string",
      "x-prompt": "Which module should we generate the container in?"
    }
  },
  "required": [
    "name",
    "module"
  ]
}
